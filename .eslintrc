{
    root: true,
    parser: 'babel-eslint',
    extends: ['eslint:recommended'],
    env: {
        commonjs: true,
        node: true,
        browser: true,
    },
    parserOptions: {
        sourceType: 'module'
    },
    rules: {
      'no-unused-vars': 'off',
      'no-console': 'off'
    }
}
